<p id="notice"><%= notice %></p>

<h1>Trackings</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Uuid</th>
      <th>Created At</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trackings.each do |tracking| %>
      <tr>
        <td><%= tracking.url %></td>
        <td><%= tracking.uuid %></td>
        <td><%= tracking.created_at.to_s(:post) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
